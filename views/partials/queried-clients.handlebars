  <table class="table table-hover">
    <thead>
      <tr>
        <th scope="col">Name</th>
        <th scope="col">E-mail</th>
        <th scope="col">Phone</th>
        <th scope="col">Alt Phone</th>
        <!--th scope="col">Clinician</th-->
      </tr>
    </thead>
    <tbody>
      {{#each clients}}
      <tr>
        <td>
          <a href="/clients/{{ client_id }}">
            {{ first_name }} {{ last_name }}
          </a>
        </td>
        <td>{{ email }}</td>
        <td>{{ primary_phone }}</td>
        <td>{{ alt_phone }}</td>
        <!--td></td-->
      </tr>
      {{/each}}
    </tbody>
  </table>