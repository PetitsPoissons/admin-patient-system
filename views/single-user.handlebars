<article class="row d-block w-100 mt-5">
<form>
  <h3>Personal Information</h3>
  <div class="form-row">
    <div class="form-group col-md-6">
      <label for="userFirstName">First Name</label>
      <input type="text" class="form-control" id="userFirstName" value="{{ user.first_name }}" />
    </div>
    <div class="form-group col-md-6">
      <label for="userLastName">Last Name</label>
      <input type="text" class="form-control" id="userLastName" value="{{ user.last_name }}" />
    </div>
  </div>
  <div class="form-row">
    <div class="form-group col-md-6">
      <label for="userDob">Date of Birth</label>
      <input type="date" class="form-control" id="userDob" value="{{ user.dob }}" />
    </div>
    <div class="form-group col-md-6">
      <label for="userSsn">Social Security #</label>
      <input type="text" class="form-control" id="userSsn" value="{{ user.ssn }}" />
    </div>
  </div>
  <div class="form-row">
    <div class="form-group col-md-6">
      <label for="userUsername">Username</label>
      <input type="text" class="form-control" id="userUsername" value="{{ user.username }}" />
    </div>
    <div class="form-group col-md-6">
      <label for="userPassword">Password</label>
      <input type="password" class="form-control" id="userPassword" value="{{ user.password }}" />
    </div>
  </div>
  <div class="form-row">
    <div class="form-group col-md-2">
      <div class="form-check">
        <input class="form-check-input" type="checkbox" id="userActive" value={{ user.active }}>
        <label class="form-check-label" for="userActive">
          Active
        </label>
      </div>
    </div>
    <div class="form-group col-md-4">
      <label for="userAccessType">Access Type</label>
      <input type="text" class="form-control" id="userAccessType" value="{{ user.access.access_type }}" />
    </div>
    <div class="form-group col-md-6">
      <label for="inputUserAccessDesc">Description</label>
      <input type="text" class="form-control" id="inputUserAccessDesc" value="{{ user.access.access_desc }}" />
    </div>
  </div>
  <h3>Contact Information</h3>
  <div class="form-group">
    <label for="inputUserEmail">Email</label>
    <input type="email" class="form-control" id="inputUserEmail" value="{{ user.email }}">
  </div>
  <div class="form-row">
    <div class="form-group col-md-6">
      <label for="inputUserPrimaryPhone">Mobile Phone</label>
      <input type="phone" class="form-control" id="inputUserPrimaryPhone" value="{{ user.primary_phone }}" />
    </div>
    <div class="form-group col-md-6">
      <label for="inputUserAltPhone">Other Phone</label>
      <input type="phone" class="form-control" id="inputUserPassword" value="{{ user.alt_phone }}" />
    </div>
  </div>
  <h4>Address:</h4>
  <div class="form-group">
    <label for="inputUserAddress">Street Address</label>
    <input type="text" class="form-control" id="inputUserAddress" value="{{ user.street_address }}">
  </div>
  <div class="form-row">
    <div class="form-group col-md-6">
      <label for="inputUserCity">City</label>
      <input type="text" class="form-control" id="inputUserCity" value="{{ user.city }}">
    </div>
    <div class="form-group col-md-4">
      <label for="inputUserState">State</label>
      <select id="inputUserState" class="form-control">
        <option selected>{{ user.state }}</option>
        <option>...</option>
      </select>
    </div>
    <div class="form-group col-md-2">
      <label for="inputUserZip">Zip</label>
      <input type="text" class="form-control" id="inputUserZip" value="{{ user.zip }}">
    </div>
  </div>
  <h3>Clinician Information</h3>
  <button type="submit" class="btn btn-primary">Save</button>
</form>
</article>
<script src="/js/edit-user.js"></script>
<script src="/js/delete-user.js"></script>