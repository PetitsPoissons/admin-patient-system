<div class="container my-3">
  <table class="table table-hover">
    <thead>
      <tr>
        <th scope="col">Name</th>
        <th scope="col">E-mail</th>
        <th scope="col">Phone</th>
        <th scope="col">Access Level</th>
      </tr>
    </thead>
    <tbody>
      {{#each users}}
      <tr>
        <td>{{ first_name }} {{ last_name }}</td>
        <td>{{ email }}</td>
        <td>{{ primary_phone }}</td>
        <td>{{ access.access_type }}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>