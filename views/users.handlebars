<div class="container my-3">
  <div class="row mt-5 mb-3 d-flex justify-content-between">
    <h4 class="my-5">Users List</h4>
    <button type="button" class="my-5 btn btn-general shadow text-white" onClick="goToNewUserPage()">+ Add User</button>
  </div>
  <table class="table table-hover">
    <thead>
      <tr>
        <th scope="col">Name</th>
        <th scope="col">E-mail</th>
        <th scope="col">Phone</th>
        <th scope="col">Access Level</th>
      </tr>
    </thead>
    <tbody>
      {{#each users}}
      <tr>
        <td>
          <a href="/users/{{ user_id }}">
            {{ last_name }}, {{ first_name }}
          </a>
        </td>
        <td>{{ email }}</td>
        <td>{{ primary_phone }}</td>
        <td>{{ access.access_type }}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>
<script>
  function goToNewUserPage() {
    document.location.replace('/users/new');
  }
</script>