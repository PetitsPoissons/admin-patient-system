<div class="container my-3">
  <div class="row mt-5 mb-3 d-flex justify-content-between">
    <h4>Users List</h4>
    <button type="button" class="btn btn-general shadow text-white" onClick="goToNewUserPage()">+ Add User</button>
  </div>
  <table class="table table-hover">
    <thead>
      <tr>
        <th scope="col">Name</th>
        <th scope="col">E-mail</th>
        <th scope="col">Phone</th>
        <th scope="col">Access Level</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      {{#each users}}
      <tr>
        <td>
          <a href="/users/{{ user_id }}">
            {{ last_name }}, {{ first_name }}
          </a>
        </td>
        <td><a href="mailto:{{ email }}">{{ email }}</a></td>
        <td><a href="tel:{{ primary_phone }}">{{ primary_phone }}</a></td>
        <td>{{ access.access_type }}</td>
        <td><button type="submit" class="btn btn-delete bg-default"><i class="fa fa-trash text-danger"></i></button></td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>
<script>
  function goToNewUserPage() {
    document.location.replace('/users/new');
  }
</script>