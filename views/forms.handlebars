<div class="container my-3">
  <table class="table table-hover">
    <thead>
      <tr>
        <th scope="col">Name</th>
        <th scope="col">Description</th>
      </tr>
    </thead>
    <tbody>
      {{#each forms}}
      <tr>
        <td>{{ form_name }}</td>
        <td>{{ form_desc }}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>